  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "start": "nodemon --inspect dist/server.js",
    "dev:server": "ts-node-dev --respawn --transpile-only --ignore node_modules src/server.ts",
    "dev:db-force": "NODE_OPTIONS='--trace-deprecation' ts-node-dev --inspect=9229 --respawn --transpile-only --ignore node_modules src/server.ts --force=true --logging=true",
    "dev:db-alter": "NODE_OPTIONS='--trace-deprecation' ts-node-dev --inspect=9229 --respawn --transpile-only --ignore node_modules src/server.ts --alter=true --logging=true",
    "pretest": "NODE_ENV=test sequelize db:migrate && NODE_ENV=test sequelize db:seed:all",
    "test": "NODE_ENV=test jest",
    "posttest": "NODE_ENV=test sequelize db:migrate:undo:all",
    "db:migrate": "rm -Rf dist && npm run build && npx sequelize db:migrate",
    "db:seed": "npx sequelize db:seed:all",
    "obfuscate": "rm -Rf dist && npm run build && node obfuscate.js",
    "backup": "ts-node-dev  --respawn --transpile-only --ignore node_modules src/backup_restore.ts backup",
    "restore": "ts-node-dev  --respawn --transpile-only --ignore node_modules src/backup_restore.ts restore",
    "dbcreate": "ts-node-dev  --respawn --transpile-only --ignore node_modules src/backup_restore.ts dbcreate"
  },
